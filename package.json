{
  "name": "@janiscommerce/model-test",
  "version": "1.0.0",
  "description": "A package to test Janis Models",
  "main": "lib/model-test.js",
  "scripts": {
    "test": "export TEST_ENV=true; mocha --exit -R nyan --recursive tests/",
    "test-ci": "nyc --reporter=lcov --reporter=text-summary mocha --exit --recursive tests/",
    "watch-test": "export TEST_ENV=true; mocha --exit -R nyan -w --recursive tests/",
    "coverage": "nyc npm test",
    "lint": "eslint lib/ tests/",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janis-commerce/model-test.git"
  },
  "author": "Janis",
  "license": "ISC",
  "homepage": "https://github.com/janis-commerce/model-test.git#readme",
  "devDependencies": {
    "@janiscommerce/model": "^5.6.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^7.0.2",
    "mocha": "^9.1.2",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2"
  },
  "dependencies": {
    "lllog": "^1.1.2",
    "superstruct": "0.6.1"
  },
  "files": [
    "lib/"
  ],
  "directories": {
    "test": "tests"
  }
}
